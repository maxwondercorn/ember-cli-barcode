{
  "name": "ember-cli-barcode",
  "version": "0.3.12",
  "description": "Ember barcode generator using the JSBarcode library.",
  "keywords": [
    "ember-addon",
    "barcode",
    "jsbarcode"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maxwondercorn/ember-cli-barcode"
  },
  "license": "MIT",
  "author": "Max Wondercorn aka Gregg Martell",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "contributors": [
    "christopher-noq <> (https://github.com/christopher-noq?tab=repositories)",
    "donaldwasserman <> (http://donaldwasserman.com)"
  ],
  "ember-addon": {
    "demoURL": "https://maxwondercorn.github.io/ember-cli-barcode/",
    "configPath": "tests/dummy/config"
  },
  "scripts": {
    "build": "ember build",
    "changelog": "npx auto-changelog -u --output CHANGELOG.md --commit-limit false",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "deploy": "ember build && ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "changelog-template.hbs",
    "unreleased": true,
    "commitLimit": false
  },
  "dependencies": {
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^3.0.2",
    "ember-cli-babel": "^7.13.0",
    "jsbarcode": "^3.11.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.1.0",
    "auto-changelog": "^1.16.2",
    "babel-eslint": "^10.0.3",
    "bootstrap": "^4.3.1",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.12.0",
    "ember-cli-bootstrap-4": "^0.11.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-htmlbars": "^4.0.8",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-cli-uglify": "^3.0.0",
    "ember-decorators": "^6.1.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^6.0.0",
    "ember-source": "~3.12.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "^1.3.0",
    "eslint-plugin-ember": "^7.6.0",
    "eslint-plugin-node": "^10.0.0",
    "loader.js": "^4.7.0",
    "popper.js": "^1.16.0",
    "qunit-dom": "^0.9.2",
    "sass": "^1.23.7"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  }
}
